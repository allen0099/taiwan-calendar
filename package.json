{
  "name": "taiwan-calendar",
  "version": "1.0.0",
  "description": "Taiwan government holiday calendar API - Auto-updated from Executive Yuan Personnel Administration. Data licensed under Open Government Data License.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "fetch": "tsx src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "taiwan",
    "calendar",
    "holiday",
    "government",
    "api",
    "open-data"
  ],
  "author": "allen0099",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/allen0099/taiwan-calendar.git"
  },
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "csv-parse": "^6.1.0",
    "iconv-lite": "^0.7.0"
  },
  "devDependencies": {
    "@types/iconv-lite": "^0.0.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
