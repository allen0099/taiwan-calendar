{
  "name": "taiwan-calendar",
  "version": "1.0.0",
  "description": "Taiwan government holiday calendar API - Auto-updated from Executive Yuan Personnel Administration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "fetch": "tsx src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "taiwan",
    "calendar",
    "holiday",
    "government",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "csv-parse": "^5.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
